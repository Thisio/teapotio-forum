{% extends "TeapotioSiteBundle:partial:base.html.twig" %}

{% block Partial_class %}{{ parent() }} Partial--userSignup{% endblock %}

{% block Partial_content %}
  <form action="{{ path("TeapotioBaseUserBundle_signup") }}" method="post" {{ form_enctype(form) }}>
    <h2>{% trans %}Sign.up.message{% endtrans %}</h2>

    {% include "TeapotioUserBundle:component:form/signup.html.twig" with {form: form} %}
  </form>
{% endblock %}
